  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "PageView",
      "runtimeArgs": ["-r", "ts-node/register"],
      "args": ["src/index.ts"],
      "outputCapture": "std",
      "cwd": "${workspaceRoot}",
      "env": {
        "TS_NODE_FILES": "true",
        "NODE_ENV": "development"
      }
    }
  ]
}
